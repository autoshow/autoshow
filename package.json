{
  "name": "autoshow-bun",
  "type": "module",
  "author": "Anthony Campolo",
  "license": "MIT",
  "scripts": {
    "repo": "bash .github/repomix.sh",
    "check": "bunx tsc --noEmit",
    "as": "bun --env-file=.env scripts/cli.ts",
    "dev": "bun run --bun vinxi dev",
    "build": "bun run --bun vinxi build",
    "start": "bun run --bun vinxi start",
    "h": "bun as help",
    "up": "bun as up",
    "up prune": "bun as up prune",
    "prune": "bun as prune",
    "in": "bun as info",
    "docker-report": "bun as docker-report",
    "build-report": "bun as build-report",
    "test": "bun test",
    "test:api": "bun test tests/api/"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.71.2",
    "@elevenlabs/elevenlabs-js": "2.32.0",
    "@google/genai": "1.38.0",
    "@solidjs/meta": "0.29.4",
    "@solidjs/router": "0.15.4",
    "@solidjs/start": "1.2.1",
    "marked": "17.0.1",
    "openai": "6.16.0",
    "solid-js": "1.9.10",
    "valibot": "1.2.0",
    "vinxi": "0.5.11"
  },
  "devDependencies": {
    "@types/bun": "1.3.6",
    "playwright": "1.57.0",
    "repomix": "1.11.1",
    "typescript": "5.9.3"
  }
}
