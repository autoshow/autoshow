{
  "name": "autoshow-bun",
  "type": "module",
  "author": "Anthony Campolo",
  "license": "MIT",
  "scripts": {
    "repo": "bash .github/repomix.sh",
    "check": "bunx tsc --noEmit",
    "as": "bun scripts/cli.ts",
    "dev": "bun run --bun vinxi dev",
    "build": "bun run --bun vinxi build --preset bun",
    "start": "bun run --bun vinxi start --preset bun",
    "h": "bun as help",
    "up": "bun as up",
    "up prune": "bun as up prune",
    "prune": "bun as prune",
    "in": "bun as info",
    "docker-report": "bun as docker-report",
    "build-report": "bun as build-report",
    "test": "bun test",
    "test:api": "bun test tests/api",
    "test:health": "bun test tests/api/health.test.ts",
    "test:e2e": "bun test tests/e2e.test.ts",
    "test:verify-base": "bun test tests/verify-base.test.ts",
    "e2e": "bun scripts/test/e2e.ts",
    "play": "bunx playwright test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.71.2",
    "@elevenlabs/elevenlabs-js": "2.33.0",
    "@google/genai": "1.38.0",
    "@solidjs/meta": "0.29.4",
    "@solidjs/router": "0.15.4",
    "@solidjs/start": "1.2.1",
    "@valibot/to-json-schema": "1.5.0",
    "openai": "6.16.0",
    "solid-js": "1.9.11",
    "valibot": "1.2.0",
    "vinxi": "0.5.11"
  },
  "devDependencies": {
    "@playwright/test": "1.58.0",
    "@types/bun": "1.3.6",
    "playwright": "1.58.0",
    "repomix": "1.11.1",
    "typescript": "5.9.3"
  }
}
